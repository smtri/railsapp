<%= link_to 'Back', api_v1_user_path(@user) %>
<p>
<%= image_tag @image.image unless @image.image.blank? %>
</p>
<%= link_to 'Rotate image', api_v1_user_image_path(@user.id, @image.id), method: :update %>
<%= link_to 'Delete', api_v1_user_image_path(@user, @image), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-small btn-danger' %>
