<% if current_user %>
  <ul>
    <p><h1>Welcome, <%= current_user.name %></h1></p>
    <p><h2><%= link_to 'Users list', controller: 'users' %></h2></p>
    <p><%= link_to "Log out", logout_path, method: "delete" %></p>
  </ul>
<% else %>
  <ul>
    <li><%= link_to "Signup", "signup" %></li>
    <li><%= link_to "Login", "login" %></li>
  </ul>
<% end %>